QWidget{
	font-family:"Microsoft YaHei";
	font-size:12px;
	color:rgb(68,68,68);
}

/*分隔器*/
QLabel#divider{
	background-color:rgb(68,68,68);
}

/*所有菜单样式*/

QMenuBar {
	background-color:rgb(45,45,48);
	color: white;
}
 
QMenuBar::item {
    spacing: 50px;
    padding: 2px 10px;   
	border-width:1px 1px 0px 1px;
	border-color:transparent;
	border-style:solid;
    background: rgb(45,45,48);
 }

 QMenuBar::item:selected {
     background: rgb(62,62,64);
 }

 QMenuBar::item:pressed {
    background: rgb(27,27,28);
	border-width:1px 1px 0px 1px;
	border-color:rgb(51,51,55);
	border-style:solid;
 }

QMenuBar>QMenu{
	background-color:rgb(27,27,28);
    border: 1px solid rgb(51,51,55);
}

QMenuBar>QMenu::item {       
        color: rgb(225,225,225);
        background-color:rgb(27,27,28);
}
		
QMenuBar>QMenu::item:selected {
        background-color:rgb(51,51,52);
}	
/*菜单项*/
