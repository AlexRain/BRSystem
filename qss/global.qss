QToolButton {
    border: 0px solid #333333;
    margin: 1px;
}

QToolButton:hover {
    background-color: rgb(62,62,64);
}

QToolButton::menu-button {
    background: transparent;
    width: 16px;
}

QToolButton::menu-button:hover {
    background: transparent;
    width: 16px;
}

QToolButton:checked, QToolButton:pressed {
    background-color: rgb(0,122,204);
}

/*顶部界面*/
QPushButton#btn_changeSkin{
	border-image:url(":/images/images/icon_skin.png");
}

/*中间界面样式*/

CUiCenter{
	border-color:rgb(63,63,70);border-width:1px;border-style:solid;
}

CUiCenter>QTableView{
	background-color:rgb(255,255,255);
    alternate-background-color: rgb(232, 232, 255);
	
	border-width:1px;border-color:rgb(63,63,70);border-style:solid;
	/*color:rgb(68,68,68);*/
}

CUiCenter>QTableView::item:selected{
        /*color:white;*/
        background:rgb(192, 192, 192);    
}

QHeaderView::section {
    color: black;padding-left:4px;
	font-weight:normal;
	border:0px;
}

BRSystem{
	background-color:rgb(45,45,48);
}

/*查询控件样式*/
CSearchLineEdit{
	color:gray;
	selection-background-color:rgb(58,99,144);
	background-color:rgb(51,51,55);
	padding-right:25px;padding-left:5px;
	border-style:solid;border-color:rgb(63,63,70);border-width:1px;
}

CSearchLineEdit:hover{
	color:white;
	border-color:rgb(0,122,204);
}

CSearchLineEdit>QPushButton{
	border-image:url(:/images/images/search.png);
}